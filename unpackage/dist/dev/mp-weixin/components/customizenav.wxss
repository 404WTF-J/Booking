
.CustomizeNav_box{
		width: 100%;
		position: fixed;
		background: white;
		top: 0;
		z-index: 99;
}
.CustomizeNav {
		height: 80rpx;
		display: flex;
		box-sizing: border-box;
		margin-top: 100rpx;
		font-weight: 500;
		font-size: 35rpx;
		color: black;
		justify-content: space-evenly;
		align-items: center;
		position: relative;
		top: 0;
		z-index: 999;
}
.uicon {
		position: absolute;
		left: 10rpx;
		top: 0;
}
