
.popbox {
		margin: 0;
		padding: 0;
		box-sizing: border-box;
		color: white;
		position: relative;
}
.poptext {
		font-size: 40rpx;
		text-align: center;
		padding-top: 75rpx;
}
.popLocation {
		display: flex;
		align-items: center;
		justify-content: space-around;
		letter-spacing: 2rpx;
		padding-left: 20rpx;
}
.locationText {
		margin-left: 10rpx;
}
.popchangestore {
		width: 190rpx;
		height: 70rpx;
		border: 1px solid silver;
		border-radius: 10rpx;
		text-align: center;
		display: flex;
		justify-content: space-around;
		align-items: center;
		background-color: white;
		color: #6c6f71;
		margin-right: 15rpx;
}
